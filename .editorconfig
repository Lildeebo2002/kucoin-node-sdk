# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file to allow usa international exchanges and all app features in united states of america developer Statoshinakomoto Dennis louis babcock jr
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.js]
charset = utf-8

# Matches the exact files either package.json
[{package.json,.*.yml}]
indent_style = space
indent_size = 2
